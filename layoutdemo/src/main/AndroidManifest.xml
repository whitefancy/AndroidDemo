<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whitefancy.layoutdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidDemo">
        <activity
            android:name=".MyRecyclerView.RecyclerActivity"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name=".ui.interaction.InteractionActivity"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name=".ui.interaction.InteractionActivity1"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name=".ui.interaction.InteractionActivity2"
            android:theme="@style/Theme.Design.NoActionBar"></activity>
        <activity android:name=".ui.textview.TextViewActivity"></activity>
        <activity android:name=".ui.menu.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>